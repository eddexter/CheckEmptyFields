<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightning__RecordPage</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__RecordPage">
            <property name="fieldsetName" type="String" required="true" description="Nome do Fieldset"/>
            <property name="keyField" type="String" required="true" description="Nome do campos que será retornado para o Flow para identificar o registro"/>
            <property name="addCheckbox" type="Boolean" default="false" description="Campo true validará campos booleanos adicionados devam ser marcados" />
            <property name="fromFlow" type="Boolean" default="false" description="Valida a utilização do método por Flow ou LWC" />
            <supportedFormFactors>
                <supportedFormFactor type="Large" />
                <supportedFormFactor type="Small" />
            </supportedFormFactors>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>